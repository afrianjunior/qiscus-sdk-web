{
  "name": "qiscus-sdk-web",
  "version": "1.0.0",
  "description": "Qiscus Javascript SDK For Web",
  "author": "contact.us@qiscus.com",
  "private": true,
  "scripts": {
    "dev": "node build/dev-server.js",
    "build": "node build/build.js",
    "unit": "karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "",
    "start": "node ./internals/dev-server",
    "build:production": "npm run build:production:npm && npm run build:production:umd",
    "build:production:npm": "cross-env NODE_ENV=production BUILD_ENV=production BUILD_MODE=npm webpack -c ./internals/webpack.config.js",
    "build:production:umd": "cross-env NODE_ENV=production BUILD_ENV=production BUILD_MODE=umd webpack -c ./internals/webpack.config.js",
    "build:development": "cross-env NODE_ENV=development BUILD_ENV=development BUILD_MODE=umd webpack -c ./internals/webpack.cofnig.js"
  },
  "dependencies": {
    "babel-runtime": "^6.0.0",
    "embed-js": "^4.1.4",
    "highlight.js": "^9.6.0",
    "lodash": "^4.15.0",
    "marked": "^0.3.6",
    "moment": "^2.14.1",
    "superagent": "^2.2.0",
    "vue": "^1.0.21",
    "vuex": "^1.0.0-rc.2"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "css-loader": "^0.23.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.8.1",
    "json-loader": "^0.5.4",
    "node-sass": "^3.8.0",
    "sass-loader": "^4.0.1",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.2.0",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^8.3.0",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
